{
  "name": "coolhole-chatbot",
  "version": "1.0.0",
  "description": "A Node.js chatbot for Coolhole.org CyTube integration with CoolPoints system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "start:voice": "node src/voice-service/index.js",
    "check": "node check.js",
    "setup": "npm install && node check.js",
    "deploy-24-7": "node production-slunt-24-7.js",
    "deploy-advanced": "node advanced-slunt-learning.js",
    "deploy-visual": "node visual-slunt-monitor.js",
    "deploy-fast": "node fast-visual-slunt.js",
    "deploy-simple": "node visual-slunt-simple.js",
    "test:17": "node coolhole-slunt-17min.js"
  },
  "keywords": [
    "chatbot",
    "cytube",
    "websocket",
    "coolpoints",
    "coolhole"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/vision": "^5.3.4",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "discord.js": "^14.24.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "helmet": "^7.1.0",
    "jimp": "^1.6.0",
    "node-cron": "^4.2.1",
    "ollama": "^0.6.0",
    "openai": "^6.7.0",
    "playwright": "^1.56.1",
    "puppeteer": "^24.28.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tesseract.js": "^6.0.1",
    "tmi.js": "^1.8.5",
    "winston": "^3.18.3",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
