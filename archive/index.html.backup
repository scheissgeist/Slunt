    <!-- Insights Modal -->
    <div id="insightsModal" style="display: none; position: fixed; top: 0; left: 0; width: 100%; height: 100%; background: rgba(0,0,0,0.8); z-index: 1000; justify-content: center; align-items: center;">
        <div style="background: #1e293b; border-radius: 12px; padding: 30px; max-width: 700px; max-height: 80vh; overflow-y: auto; position: relative;">
            <button onclick="closeInsightsModal()" style="position: absolute; top: 15px; right: 15px; background: rgba(255,255,255,0.1); border: none; color: white; font-size: 24px; width: 35px; height: 35px; border-radius: 50%; cursor: pointer; line-height: 1;">√ó</button>
            <h2 id="modalTitle" style="margin-top: 0; margin-bottom: 20px;"></h2>
            <div id="modalContent" style="font-size: 0.95em; line-height: 1.6;"></div>
        </div>
    </div>

    <script>
        // Socket.IO connection
        const socket = io('http://localhost:3001', {
            transports: ['websocket', 'polling']
        });

        const state = {
            startTime: Date.now(),
            chatHistory: [],
            maxChatHistory: 50,
            advancedStats: null,
            seenMessages: new Set() // Track seen messages to prevent duplicates
        };

        // Connection status
        socket.on('connect', () => {
            console.log('‚úÖ Socket.IO connected');
            updateConnectionStatus(true);
        });

        socket.on('disconnect', () => {
            console.log('‚ùå Socket.IO disconnected');
            updateConnectionStatus(false);
        });

        function updateConnectionStatus(connected) {
            const statusEl = document.getElementById('connectionStatus');
            const textEl = document.getElementById('connectionText');
            
            if (connected) {
                statusEl.className = 'connection-status status-badge online pulse';
                textEl.textContent = 'üü¢ Live';
            } else {
                statusEl.className = 'connection-status status-badge offline';
                textEl.textContent = 'üî¥ Disconnected';
            }
        }

        // Update uptime
        setInterval(() => {
            const minutes = Math.floor((Date.now() - state.startTime) / 60000);
            const hours = Math.floor(minutes / 60);
            const mins = minutes % 60;
            
            let uptimeText = hours > 0 ? `${hours}h ${mins}m` : `${mins}m`;
            document.getElementById('uptime').textContent = `Uptime: ${uptimeText}`;
        }, 10000);

        // Chat event
        socket.on('chat:message', (data) => {
            console.log('üì® Chat message:', data);
            
            // Create unique ID for deduplication
            const msgId = `${data.username}-${data.message}-${Math.floor(data.timestamp / 1000)}`;
            if (state.seenMessages.has(msgId)) {
                console.log('‚è≠Ô∏è Skipping duplicate message');
                return;
            }
            state.seenMessages.add(msgId);
            
            // Clean up old message IDs (keep last 100)
            if (state.seenMessages.size > 100) {
                const arr = Array.from(state.seenMessages);
                state.seenMessages = new Set(arr.slice(-100));
            }
            
            addChatMessage(data);
            updateLastUpdate();
        });

        // Bot stats update
        socket.on('bot:stats', (stats) => {
            console.log('üìä Bot stats:', stats);
            state.advancedStats = stats.advanced; // Store for insights modal
            updateBotStats(stats);
            updateLastUpdate();
        });

        // Vision events
        socket.on('vision:screenshot', (data) => {
            console.log('üì∏ Vision screenshot:', data);
            updateVisionAnalysis(data);
            updateLastUpdate();
        });

        // Explorer events
        socket.on('explorer:emotes', (emotes) => {
            console.log('üòä Emotes discovered:', emotes);
            updateEmotes(emotes);
            updateLastUpdate();
        });

        socket.on('explorer:commands', (commands) => {
            console.log('‚ö° Commands discovered:', commands);
            updateCommands(commands);
            updateLastUpdate();
        });

        socket.on('explorer:discovery', (data) => {
            console.log('üîç Explorer discovery:', data);
            updateExplorerStats(data);
            updateLastUpdate();
        });

        // User profile updates
        socket.on('bot:user_profile', (profile) => {
            console.log('üë§ User profile update:', profile);
            updateUserProfile(profile);
            updateLastUpdate();
        });

        // Community insights
        socket.on('bot:community_insights', (insights) => {
            console.log('üìä Community insights:', insights);
            updateCommunityInsights(insights);
            updateLastUpdate();
        });

        // Cursor events
        socket.on('cursor:discovery', (data) => {
            console.log('üñ±Ô∏è Cursor discovery:', data);
            updateCursorDiscovery(data);
            updateLastUpdate();
        });

        socket.on('cursor:emoteUsed', (data) => {
            console.log('üòä Cursor emote used:', data);
            updateCursorEmoteUsed(data);
            updateLastUpdate();
        });

        socket.on('cursor:interaction', (data) => {
            console.log('üñ±Ô∏è Cursor interaction:', data);
            updateCursorInteraction(data);
            updateLastUpdate();
        });

        function addChatMessage(data) {
            const container = document.getElementById('chatMessages');
            const isBot = data.username === 'Slunt' || data.username === 'slunt';
            
            const msgDiv = document.createElement('div');
            msgDiv.className = `chat-message ${isBot ? 'bot' : ''}`;
            msgDiv.innerHTML = `
                <span class="timestamp">[${new Date().toLocaleTimeString()}]</span>
                <span class="username">${data.username}:</span>
                <span>${escapeHtml(data.message)}</span>
            `;
            
            container.insertBefore(msgDiv, container.firstChild);
            
            // Keep only recent messages
            while (container.children.length > state.maxChatHistory) {
                container.removeChild(container.lastChild);
            }

            // Remove placeholder
            const placeholder = container.querySelector('[style*="text-align: center"]');
            if (placeholder) placeholder.remove();
        }

        function updateBotStats(stats) {
            document.getElementById('totalMessages').textContent = stats.totalMessages || 0;
            document.getElementById('messagesSent').textContent = stats.messagesSent || 0;
            document.getElementById('questionsAsked').textContent = stats.questionsAsked || 0;
            document.getElementById('usersTracked').textContent = stats.usersTracked || 0;
            document.getElementById('topicsLearned').textContent = stats.topicsLearned || 0;
            
            const responseRate = stats.totalMessages > 0 
                ? Math.round((stats.messagesSent / stats.totalMessages) * 100)
                : 0;
            document.getElementById('responseRate').textContent = `${responseRate}%`;

            // Update personality traits
            if (stats.personality) {
                const traits = ['humor', 'snarkiness', 'edginess', 'chaos', 'supportiveness', 
                               'empathy', 'creativity', 'wisdom', 'spontaneity', 'loyalty'];
                traits.forEach(trait => {
                    if (stats.personality[trait] !== undefined) {
                        updatePersonalityTrait(trait, stats.personality[trait]);
                    }
                });
            }

            // === NEW: UPDATE ADVANCED SYSTEMS STATS ===
            if (stats.advanced) {
                document.getElementById('emotionalCount').textContent = stats.advanced.emotional || 0;
                document.getElementById('relationshipCount').textContent = 
                    (stats.advanced.relationships && stats.advanced.relationships.totalRelationships) || 0;
                document.getElementById('videoCount').textContent = 
                    (stats.advanced.video && stats.advanced.video.totalVideosWatched) || 0;
                document.getElementById('memoryCount').textContent = 
                    (stats.advanced.memory && stats.advanced.memory.totalMemories) || 0;
                document.getElementById('personalityEvolutions').textContent = 
                    (stats.advanced.personality && stats.advanced.personality.totalEvolutions) || 0;
                document.getElementById('socialHealth').textContent = 
                    (stats.advanced.social && stats.advanced.social.chatHealth && stats.advanced.social.chatHealth.activity) || 'Normal';
                document.getElementById('userRelationshipCount').textContent = stats.usersTracked || 0;
            }

            // Update status badges
            document.getElementById('botStatus').className = 'status-badge online';
            document.getElementById('botStatus').textContent = 'Bot: Online';
            
            // Update mental state if available
            if (stats.advanced && stats.advanced.mentalState) {
                updateMentalState(stats.advanced.mentalState);
            }
            
            // === NEW: Update Psychological Depth Systems ===
            if (stats.advanced && stats.advanced.psychological) {
                updatePsychologicalSystems(stats.advanced.psychological);
            }
        }
        
        function updatePsychologicalSystems(psych) {
            // Memory Decay
            if (psych.memory) {
                document.getElementById('memoryCount').textContent = psych.memory.totalMemories || 0;
                document.getElementById('memoryClarity').textContent = 
                    `${(psych.memory.averageClarity * 100).toFixed(0)}%`;
            }
            
            // Obsession System
            if (psych.obsession && psych.obsession.isObsessed) {
                document.getElementById('obsessionStat').style.display = 'flex';
                document.getElementById('obsessionIntensity').style.display = 'flex';
                document.getElementById('currentObsession').textContent = psych.obsession.topic || '-';
                document.getElementById('obsessionLevel').textContent = 
                    `${(psych.obsession.intensity * 100).toFixed(0)}%`;
            } else {
                document.getElementById('obsessionStat').style.display = 'none';
                document.getElementById('obsessionIntensity').style.display = 'none';
            }
            
            // Grudge System
            if (psych.grudge && psych.grudge.activeGrudges > 0) {
                document.getElementById('grudgeStat').style.display = 'flex';
                document.getElementById('grudgeCount').textContent = psych.grudge.activeGrudges || 0;
            } else {
                document.getElementById('grudgeStat').style.display = 'none';
            }
            
            // Drunk Mode
            if (psych.drunk && psych.drunk.isDrunk) {
                document.getElementById('drunkStat').style.display = 'flex';
                document.getElementById('drunkLevel').textContent = 
                    `${(psych.drunk.drunkLevel * 100).toFixed(0)}%`;
                document.getElementById('hangoverStat').style.display = 'none';
            } else if (psych.drunk && psych.drunk.hasHangover) {
                document.getElementById('drunkStat').style.display = 'none';
                document.getElementById('hangoverStat').style.display = 'flex';
                document.getElementById('hangoverTime').textContent = 
                    `${psych.drunk.minutesLeft}m left`;
            } else {
                document.getElementById('drunkStat').style.display = 'none';
                document.getElementById('hangoverStat').style.display = 'none';
            }
            
            // Theory of Mind
            if (psych.theoryOfMind) {
                document.getElementById('trackedUsers').textContent = psych.theoryOfMind.activeUsers || 0;
                document.getElementById('explanationsGiven').textContent = 
                    psych.theoryOfMind.explanationsGiven || 0;
            }
        }
        
        function updateMentalState(mental) {
            // Update overall state
            const overallState = mental.overallState;
            const statusEl = document.getElementById('mentalStateStatus');
            const subtextEl = document.getElementById('mentalStateSubtext');
            const cardEl = document.getElementById('mental-state-card');
            const titleEl = document.getElementById('mental-state-title');
            
            // State display
            const stateEmojis = {
                'thriving': 'üåü',
                'doing_okay': 'üòä',
                'struggling': 'üòê',
                'depressed': 'üòî',
                'crisis': 'üíÄ'
            };
            const stateColors = {
                'thriving': '#10b981',
                'doing_okay': '#3b82f6',
                'struggling': '#f59e0b',
                'depressed': '#ef4444',
                'crisis': '#7f1d1d'
            };
            
            statusEl.textContent = `${stateEmojis[overallState] || 'üß†'} ${overallState.replace('_', ' ').toUpperCase()}`;
            statusEl.style.color = stateColors[overallState] || '#fff';
            subtextEl.textContent = 'Current mental state';
            cardEl.style.background = 'rgba(0, 0, 0, 0.2)';
            cardEl.style.border = 'none';
            cardEl.style.boxShadow = 'none';
            titleEl.textContent = 'üß† Mental State';
            
            // Update individual metrics
            document.getElementById('depressionLevel').textContent = `${(mental.depression * 100).toFixed(0)}%`;
            document.getElementById('anxietyLevel').textContent = `${(mental.anxiety * 100).toFixed(0)}%`;
            document.getElementById('confidenceLevel').textContent = `${(mental.confidence * 100).toFixed(0)}%`;
            document.getElementById('existentialDreadLevel').textContent = `${(mental.existentialDread * 100).toFixed(0)}%`;
            document.getElementById('lonelinessLevel').textContent = `${(mental.loneliness * 100).toFixed(0)}%`;
            
            // Color code metrics (red = bad, green = good)
            const depressionEl = document.getElementById('depressionLevel');
            depressionEl.style.color = mental.depression > 0.7 ? '#ef4444' : mental.depression > 0.4 ? '#f59e0b' : '#10b981';
            
            const anxietyEl = document.getElementById('anxietyLevel');
            anxietyEl.style.color = mental.anxiety > 0.7 ? '#ef4444' : mental.anxiety > 0.4 ? '#f59e0b' : '#10b981';
            
            const confidenceEl = document.getElementById('confidenceLevel');
            confidenceEl.style.color = mental.confidence < 0.3 ? '#ef4444' : mental.confidence < 0.6 ? '#f59e0b' : '#10b981';
            
            const dreadEl = document.getElementById('existentialDreadLevel');
            dreadEl.style.color = mental.existentialDread > 0.7 ? '#ef4444' : mental.existentialDread > 0.4 ? '#f59e0b' : '#10b981';
            
            const lonelinessEl = document.getElementById('lonelinessLevel');
            lonelinessEl.style.color = mental.loneliness > 0.7 ? '#ef4444' : mental.loneliness > 0.4 ? '#f59e0b' : '#10b981';
        }

        function updatePersonalityTrait(trait, value) {
            const percentage = Math.round(value * 100);
            document.getElementById(trait).style.width = `${percentage}%`;
            document.getElementById(`${trait}Value`).textContent = value.toFixed(2);
        }

        function updateVisionAnalysis(data) {
            document.getElementById('lastCapture').textContent = new Date().toLocaleTimeString();
            document.getElementById('totalCaptures').textContent = (parseInt(document.getElementById('totalCaptures').textContent) || 0) + 1;
            
            if (data.screenshot) {
                const img = document.getElementById('latestScreenshot');
                img.src = data.screenshot;
                img.style.display = 'block';
            }

            if (data.ocrText) {
                const ocrDiv = document.getElementById('ocrText');
                ocrDiv.textContent = data.ocrText;
                ocrDiv.style.display = 'block';
            }

            document.getElementById('visionStatus').className = 'status-badge online';
            document.getElementById('visionStatus').textContent = 'Vision: Active';
        }

        function updateEmotes(emotes) {
            const container = document.getElementById('emotesList');
            container.innerHTML = '';
            
            emotes.slice(0, 20).forEach(emote => {
                const div = document.createElement('div');
                div.className = 'emote-item';
                div.textContent = emote.name || emote;
                container.appendChild(div);
            });

            document.getElementById('emotesFound').textContent = emotes.length;
        }

        function updateCommands(commands) {
            const container = document.getElementById('commandsList');
            container.innerHTML = '';
            
            commands.forEach(cmd => {
                const div = document.createElement('div');
                div.className = 'command-item';
                div.textContent = cmd.name || cmd;
                container.appendChild(div);
            });

            document.getElementById('commandsFound').textContent = commands.length;
        }

        function updateExplorerStats(data) {
            if (data.emotes) document.getElementById('emotesFound').textContent = data.emotes;
            if (data.commands) document.getElementById('commandsFound').textContent = data.commands;
            if (data.uiElements) document.getElementById('uiElements').textContent = data.uiElements;

            document.getElementById('explorerStatus').className = 'status-badge online';
            document.getElementById('explorerStatus').textContent = 'Explorer: Active';
        }

        function updateUserProfile(profile) {
            const container = document.getElementById('activeUsers');
            let userEl = document.getElementById(`user-${profile.username}`);
            
            if (!userEl) {
                // Remove placeholder
                const placeholder = container.querySelector('[style*="text-align: center"]');
                if (placeholder) placeholder.remove();

                userEl = document.createElement('div');
                userEl.id = `user-${profile.username}`;
                userEl.className = 'user-card';
                container.appendChild(userEl);
            }

            userEl.innerHTML = `
                <div class="username">${profile.username}</div>
                <div class="user-stats">
                    <span>üí¨ ${profile.messageCount || 0}</span>
                    <span>‚ù§Ô∏è ${profile.friendship || 0}/100</span>
                </div>
            `;
            
            // Update active user count
            const userCount = container.querySelectorAll('.user-card').length;
            document.getElementById('activeUserCount').textContent = userCount;
        }

        function updateCommunityInsights(insights) {
            if (insights.peakTime) {
                document.getElementById('peakTime').textContent = insights.peakTime;
            }

            if (insights.popularTopics && insights.popularTopics.length > 0) {
                const topicsDiv = document.getElementById('popularTopics');
                topicsDiv.innerHTML = insights.popularTopics.slice(0, 5)
                    .map(topic => `<div style="margin: 3px 0;">‚Ä¢ ${topic}</div>`)
                    .join('');
            }

            if (insights.chatVelocity !== undefined) {
                document.getElementById('chatVelocity').textContent = `${insights.chatVelocity} msg/min`;
            }
        }

        function updateCursorDiscovery(data) {
            document.getElementById('cursorDiscovered').textContent = data.total || 0;
            document.getElementById('cursorStatus').className = 'status-badge online';
            document.getElementById('cursorStatus').textContent = 'Cursor: Active';
        }

        function updateCursorEmoteUsed(data) {
            const current = parseInt(document.getElementById('cursorEmotesUsed').textContent) || 0;
            document.getElementById('cursorEmotesUsed').textContent = current + 1;
            document.getElementById('cursorLastAction').textContent = `Used emote: ${data.emote}`;
        }

